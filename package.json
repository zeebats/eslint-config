{
  "name": "@zeebats/eslint-config",
  "version": "1.0.2",
  "description": "ESLint sharable config for Zeebats projects",
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "vue"
  ],
  "repository": "https://github.com/zeebats/eslint-config",
  "license": "MIT",
  "author": "Zeebats",
  "main": "index.js",
  "files": [
    "rules",
    "index.js",
    "typescript.js",
    "vue.js"
  ],
  "scripts": {
    "eslint": "eslint '**/*.js'",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "tape tests/*.js",
    "update:interactive": "npx npm-check -uE"
  },
  "devDependencies": {
    "lint-staged": "12.3.8",
    "simple-git-hooks": "2.7.0",
    "tape": "5.5.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-jsdoc": "^37.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "eslint-plugin-vuejs-accessibility": "^1.0.0"
  }
}
