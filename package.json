{
	"name": "@zeebats/eslint-config",
	"version": "1.3.0",
	"description": "ESLint sharable config for Zeebats projects",
	"keywords": [
		"eslint",
		"eslint-config",
		"json",
		"typescript",
		"vue"
	],
	"repository": "https://github.com/zeebats/eslint-config",
	"license": "MIT",
	"author": "Zeebats",
	"main": "index.js",
	"files": [
		"index.js",
		"rules",
		"typescript.js",
		"vue.js",
		"vue3.js"
	],
	"scripts": {
		"eslint": "eslint '{,.}**/{,.}*.{js,json,mjs}'",
		"eslint:fix": "npm run eslint -- --fix",
		"test": "tape tests/*.js",
		"update:interactive": "npx npm-check -uE"
	},
	"devDependencies": {
		"@commitlint/cli": "17.0.3",
		"@commitlint/config-conventional": "17.0.3",
		"lint-staged": "13.0.3",
		"simple-git-hooks": "2.8.0",
		"tape": "5.5.3"
	},
	"peerDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/eslint-parser": "^7.0.0",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.0",
		"eslint-import-resolver-alias": "^1.0.0",
		"eslint-import-resolver-typescript": "^2.0.0",
		"eslint-plugin-import": "^2.0.0",
		"eslint-plugin-import-newlines": "^1.0.0",
		"eslint-plugin-jsonc": "^2.0.0",
		"eslint-plugin-sort-destructure-keys": "^1.0.0",
		"eslint-plugin-unicorn": "^42.0.0",
		"eslint-plugin-vue": "^9.0.0",
		"eslint-plugin-vuejs-accessibility": "^1.0.0"
	}
}
