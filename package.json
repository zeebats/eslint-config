  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.0.0",
    "eslint": "^7.0.0",
    "eslint-plugin-jsdoc": ">=35.0.0",
    "eslint-plugin-unicorn": ">=34.0.0"
  }
