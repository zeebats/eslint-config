{
	"name": "@zeebats/eslint-config",
	"version": "1.4.0",
	"description": "ESLint sharable config for Zeebats projects",
	"keywords": [
		"eslint",
		"eslint-config",
		"json",
		"typescript",
		"vue"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zeebats/eslint-config.git"
	},
	"license": "MIT",
	"author": "Zeebats",
	"main": "index.js",
	"files": [
		"index.js",
		"astro.js",
		"rules",
		"typescript.js",
		"vue.js",
		"vue3.js"
	],
	"scripts": {
		"eslint": "eslint '{,.}**/{,.}*.{js,json,mjs}'",
		"eslint:fix": "pnpm run eslint --fix",
		"prepare": "simple-git-hooks",
		"test": "tape tests/*.js"
	},
	"devDependencies": {
		"@commitlint/cli": "19.3.0",
		"@commitlint/config-conventional": "19.2.2",
		"lint-staged": "15.2.2",
		"simple-git-hooks": "2.11.1",
		"tape": "5.7.5"
	},
	"peerDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/eslint-parser": "^7.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.0.0",
		"eslint-import-resolver-alias": "^1.0.0",
		"eslint-import-resolver-typescript": "^3.0.0",
		"eslint-plugin-astro": "^0.34.0",
		"eslint-plugin-import": "^2.0.0",
		"eslint-plugin-import-newlines": "^1.0.0",
		"eslint-plugin-jsonc": "^2.0.0",
		"eslint-plugin-jsx-a11y": "^6.0.0",
		"eslint-plugin-perfectionist": "^2.0.0",
		"eslint-plugin-unicorn": "^52.0.0",
		"eslint-plugin-vue": "^9.0.0",
		"eslint-plugin-vuejs-accessibility": "^2.0.0"
	}
}
